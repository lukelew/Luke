        <figure class="single_work" id="zhiyi_thumb">
          <a href="javascript:void(0);">
            <img src="/images/work6.jpg" alt="zhiyiVC-thumb">
          </a>
          <figcaption>zhiyiVC</figcaption>
          <h6>design + development</h6>
        </figure>
